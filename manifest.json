{
    "manifest_version": 2,
    "name": "Always Meet On Time",
    "description": "Always Meet On Time",
    "version": "1.0",
    "content_scripts": [
        {
            "js": [
                "script.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "permissions": [
        "storage"
    ],
    "icons" : {"192" : "images/1.png"},
    "browser_action": {
        "default_icon": "images/1.png",
        "default_popup": "index.html"
    }
}